# directional focus
cmd + alt - h : cycle_focus_prev
cmd + alt - l : cycle_focus_next
cmd + alt - j : cycle_focus_down
cmd + alt - k : cycle_focus_up

# directional swap
cmd + ctrl - h : yabai -m window --swap west
cmd + ctrl - l : yabai -m window --swap east
cmd + ctrl - j : yabai -m window --swap south
cmd + ctrl - k : yabai -m window --swap north

# focus recent
cmd + alt - 0x2F : yabai -m window --focus recent

# cycle layout
cmd + alt - space : cycle_layout

